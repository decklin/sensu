{
  "handlers": {
    "new_handler": {
      "type": "pipe",
      "command": "this won't override the other handlers"
    }
  },
  "env": {
    "NAME": "name",
    "CUSTOM": "custom.nested.attribute",
  },
  "checks": {
    "b": {
      "command": "ruby -e \"puts '$NAME $CUSTOM'; exit 2\"",
      "subscribers": [
        "b"
      ],
      "interval": 1,
      "auto_resolve": false
    }
  }
}
